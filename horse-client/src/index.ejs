    <link href="static/sb-admin2-4.0.7/css/sb-admin-2.min.css" rel="stylesheet">

  </head>
  <body>
    <div id="app"></div>
    <!-- Set `__static` path to static files in production -->
    <%  if (!require('process').browser) { %>
      <script>
        if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
      </script>
    <% } %>
    <!-- webpack builds are automatically injected -->


    <script src="/static/sb-admin2-4.0.7/vendor/jquery/jquery.min.js"></script>
    <script>
        $ = jQuery = module.exports;
    </script>
    <script src="/static/sb-admin2-4.0.7/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/static/sb-admin2-4.0.7/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="/static/sb-admin2-4.0.7/js/sb-admin-2.min.js"></script>

